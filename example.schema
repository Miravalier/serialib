enum Color:byte { WHITE, BLACK, RANDOM }

table LoginRequest {
    username:string = "User";
    password:[uint8];
    salt:[uint8:16];
    a_color:Color;
    b_color:Color = 2;
    admin:bool = false;
}

table Coordinate {
    x: int;
    y: int;
    z: int;
}

table LoginReply {
    username: string = "User";
    location: Coordinate;
}
